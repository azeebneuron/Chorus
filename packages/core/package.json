{
  "name": "@chorus/core",
  "version": "0.1.0",
  "description": "Core primitives for the Chorus multi-agent framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "vitest": "^4.0.17",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "keywords": [
    "ai",
    "agents",
    "llm",
    "multi-agent",
    "orchestration",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chorus-ai/chorus"
  }
}
