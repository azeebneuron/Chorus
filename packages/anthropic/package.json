    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "@chorus/core": "workspace:*"
  },
  "keywords": [
    "ai",
    "agents",
    "llm",
    "anthropic",
    "claude",
    "multi-agent",
    "chorus"
