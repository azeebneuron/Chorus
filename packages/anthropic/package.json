{
  "name": "@chorus/anthropic",
  "version": "0.1.0",
  "description": "Anthropic Claude provider for the Chorus multi-agent framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
